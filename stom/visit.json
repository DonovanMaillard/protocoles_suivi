{
  "display_properties": [
    "visit_date_min",
    "observers",
    "beginner",
    "time_start",
    "cloud_percentage",
    "rain",
    "wind",
    "visibility",
    "snow_removal"
  ],
  "display_list": [
    "visit_date_min",
    "observers",
    "beginner",
    "snow_removal"
  ],
  "specific": {
    "beginner": {
      "type_widget": "radio",
      "attribut_label": "Observateur débutant",
      "values": ["Oui", "Non"]
    },
    "time_start": {
      "type_widget": "time",
      "attribut_label": "Heure de début"
    },
    "nuages": {
      "type_widget": "select",
      "required": true,
      "attribut_label": "Couverture nuageuse",
      "values": ["0-33%", "33-66%", "66-100%"]
    },
    "pluie": {
      "type_widget": "select",
      "required": true,
      "attribut_label": "Pluie",
      "values": ["Absente", "Intermittente", "Continue"]
    },
    "vent": {
      "type_widget": "select",
      "required": true,
      "attribut_label": "Vent",
      "values": ["Absent", "Faible", "Moyen à fort"]
    },
    "visi": {
      "type_widget": "select",
      "required": true,
      "attribut_label": "Visibilité",
      "values": ["Bonne", "Modérée", "Faible"]
    },
    "deneigement": {
      "type_widget": "number",
      "required": true,
      "attribut_label": "Denneigement (en %)",
      "min": 0,
      "max": 100
    },
    "paturage": {
      "type_widget": "select",
      "attribut_label": "Indice de paturage",
      "values": [
        "1 = Aucune trace de paturage dans le milieu",
        "2 = Toutes les situations intermédiaires",
        "3 = Plus de 95% des plantes herbacées sont pâturées et rases (<10 cm)"
      ]
    },
    "medias": {
      "hidden": true
    },
    "id_dataset": {
      "hidden": true,
      "value": 2138
    },
    "comments": {
      "hidden": false,
      "attribut_label": "Commentaire sur les conditions d'écoute"
    },
    "habitat_input": {
      "type_widget": "bool_checkbox",
      "attribut_label": "Saisie habitat",
      "value": false
    },
    "roche": {
      "type_widget": "number",
      "hidden": "({value}) => !value.habitat_input",
      "attribut_label": "Rochers (%)"
    },
    "sol_nu": {
      "type_widget": "number",
      "hidden": "({value}) => !value.habitat_input",
      "attribut_label": "Sol nu (%)"
    },
    "herb": {
      "type_widget": "number",
      "hidden": "({value}) => !value.habitat_input",
      "attribut_label": "Herbacées (%)"
    },
    "arb_inf_30cm": {
      "type_widget": "number",
      "hidden": "({value}) => !value.habitat_input",
      "attribut_label": "Ligneux inf 30cm (%)"
    },
    "arb_inf_1m": {
      "type_widget": "number",
      "hidden": "({value}) => !value.habitat_input",
      "attribut_label": "Ligneux 30cm - 1m (%)"
    },
    "arb_1_4m": {
      "type_widget": "number",
      "hidden": "({value}) => !value.habitat_input",
      "attribut_label": "Ligneux 1m - 4m (%)"
    },
    "arb_sup_4m": {
      "type_widget": "number",
      "hidden": "({value}) => !value.habitat_input",
      "attribut_label": "Ligneux > 4m (%)"
    },
    "other": {
      "type_widget": "text",
      "hidden": "({value}) => !value.habitat_input",
      "attribut_label": "PAY1??"
    }
  }
}
